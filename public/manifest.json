{
  "name": "MT-rates",
  "description": "Description",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
      "16":"icons/16.png",
      "32":"icons/32.png",
      "128":"icons/128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Money transfer rates"
  },
  "permissions": [],
  "background":{
    "service_worker": "js/service_worker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ]
}
